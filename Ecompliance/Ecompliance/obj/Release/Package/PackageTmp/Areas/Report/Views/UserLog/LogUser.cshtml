
@{
    ViewBag.Title = "LogUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container maincontainer">
    <!--------------Details-------------->
    <div class="">
        <div class="top_strap">
            <div class="pull-left">
                <h3 class="page-title">User Log Report</h3>
            </div>
            <div class="clearfix"></div>
            <div style="margin:0px 0px 10px;"></div>
        </div>

        <div class="col-xs-12 col-sm-12 col-md-12">
            <div class="col-xs-12 col-sm-3 col-md-4 extra">
                <div class="col-xs-12 col-sm-4 col-md-4 allcreditionals extra">
                    <label>From Date</label>
                </div>
                <div class="col-xs-12 col-sm-8 col-md-8 allcreditionals">
                        <div class='input-group date'>
                            <input id="txtFromDate" type='text' class="form-control" placeholder="dd/mm/yyyy" />
                            <span id="spnfrmDate" class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                        @*@Html.TextBox("txtExpiryDate")*@
                </div>
            </div>

            <div class="col-xs-12 col-sm-3 col-md-4 extra">
                <div class="col-xs-12 col-sm-4 col-md-4 allcreditionals extra">
                    <label>To Date</label>
                </div>
                <div class="col-xs-12 col-sm-8 col-md-8 allcreditionals">
                        <div class='input-group date'>
                            <input id="txtToDate" type='text' class="form-control" placeholder="dd/mm/yyyy" />
                            <span id="spnToDate" class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </span>
                        </div>
                        @*@Html.TextBox("txtExpiryDate")*@
                </div>
            </div>
            <div class="col-xs-12 col-sm-2 col-md-1 extra">
                <div class="search_btn">
                    <input type="button" value="Search" class="button" id="btnSearch" />
                </div>

            </div>
        </div>
        <br />
        <br />

        <div class="col-xs-12 col-sm-12 col-md-12 pd" id="dvReport" style="display:block;">
            <div id="Kgrid"></div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12 pd" id="dvNoRecord" style="display:none;">

            <div style="width:100%;text-align:center">
                No Record Found!
            </div>
        </div>
    </div>

</div>
@section commanscript {
@Scripts.Render("~/Content/CommanScript")
<script type="text/javascript">
        var GetLogUserUrl = '@Url.RouteUrl("GetLogUser")';
</script>
@Scripts.Render("~/bundles/bndlLogUser")
}

