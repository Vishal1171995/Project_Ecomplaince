                                </div>
                                <div class="col-xs-6 col-sm-4 col-md-4 allcredit_checkbox section ">
                                    <input type="checkbox" name="group2" id="checkbox-1">
                                    <label for="checkbox-1"><span class="checkbox"></span></label>
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-12 allcreditionals">
                                    <div class="reset_btn" align="right">
                                        <input type="reset" value="Reset" class="button reset" />
                                        <input type="submit" value="Submit" class="button submit" />
                                    </div>
                                </div>

                            </div>
