<script>
$( "#dialog" ).dialog({
	autoOpen: false,
	width: 909,
	title:"Basic Details"
});

// Link to open the dialog
$( "#dialog-link" ).click(function( event ) {
	$( "#dialog" ).dialog( "open" );
	event.preventDefault();
});
</script>





